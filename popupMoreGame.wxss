.btn_default {
    background: transparent;
    padding: 0;
    margin: 0;
    border: 0;
    border-radius: 0;
    text-align: left;
    line-height: normal;
    overflow: visible;
}

.btn_default::after {
    border: none;
}

.component-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    z-index: 100;
    position: relative;
}

.title-img {
    width: 625rpx;
    height: 141rpx;
    margin-top: -70rpx;
}

.content-list {
    display: flex;
    flex-wrap: wrap;
    align-content: flex-start;
    justify-content: space-between;
    overflow-x: visible;
    overflow-y: scroll;
    padding-bottom: 60rpx;
    box-sizing: border-box;
    padding: 0 10rpx 60rpx;
}

.content-item {
    width: 184rpx;
    height: 254rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
    background: rgba(238,227,207,1);
    border: 4px solid rgba(182,164,150,1);
    border-radius: 16px;
    margin-top: 20rpx;
}

.space-item {
    width: 176rpx;
    height: 230rpx;
    border: 4px solid transparent;
}

.content-icon {
    width: 162rpx;
    height: 162rpx;
    display: block;
    border-radius: 20rpx;
    box-sizing: border-box;
    border: 4rpx solid #764e21;
    position: relative;
    overflow: hidden;
    margin: 10rpx 0 8rpx;
}

.content-name {
    font-size: 28rpx;
    font-family: SourceHanSansCN-Medium;
    font-weight: bold;
    color: rgba(72,53,38,1);
}

.continue-btn {
    width: 361rpx;
    height: 137rpx;
    display: block;
    position: absolute;
    left: 50%;
    bottom: 0;
    margin-left: -180rpx;
    transform: translateY(120%);
}

.continue-btn-image {
    width: 361rpx;
    height: 137rpx;
    display: block;
}

.btn-click {
    transform: translateY(120%) scale(0.9,0.9)!important;
    transition: all 0.1s;
}

.page-bottom {
    width: 620rpx;
    min-height: 200rpx;
    flex-shrink: 0;
    display: flex;
    align-items: flex-end;
    justify-content: center;
    margin: auto;
    margin-top: 10rpx;
}

.daoliu-banner {
    width: 100%;
}

.person-num {
    text-align: center;
    font-size: 24rpx;
    font-family: Source Han Sans CN;
    font-weight: 500;
    color: rgba(202,29,43,1);
    line-height: 1;
    margin-top: 3rpx;
}

.close-btn {
    width: 66rpx;
    height: 66rpx;
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    transform: translate(-50%,-150%);
}