}

.text1-up {
    animation: new-text1-up 1.5s ease-in-out;
    animation-fill-mode: forwards;
    line-height: 1;
}

@-webkit-keyframes new-text1-up {
    0% {
        transform: translateY(0);
        opacity: 1;
    }

    60% {
        transform: translateY(0);
        opacity: 1;
    }

    100% {
        transform: translateY(-68rpx);
        opacity: 0;
    }
}

@keyframes new-text1-up {
    0% {
        transform: translateY(0);
        opacity: 1;
    }

    60% {
        transform: translateY(0);
