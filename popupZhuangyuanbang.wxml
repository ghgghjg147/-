<popup bannerId="{{shareVideoRule.adv.scholarBanner}}" bind:close="close" notShowClose="{{true}}" showBanner="{{true}}">
    <view class="component-container">
        <view class="popup-title">
            <image class="popup-title-bg" src="/image/popup/dk.png"></image>
            <image class="popup-title-text" src="/image/popup/icon-wdzyb.png"></image>
        </view>
        <image bindtap="close" class="close-btn" src="{{fn.baseurl(siteInfo.siteroot)}}/addons/yf_chengyujiekong/resource/image/component/popupMoreGame/hbtc-qx.png"></image>
        <view class="medal-list">
            <view class="medal-item" wx:for="{{list}}" wx:key="{{index}}">
                <view bindtap="goDetail" class="item" data-week="{{item.week}}" wx:if="{{!item.isSpace}}">
                    <image src="{{item.medalUrl}}"></image>
                    <view class="medal-name" wx:if="{{item.isGet===0}}">{{item.medalName}}</view>
                </view>
                <view class="item" wx:else>
                    <image src="{{fn.baseurl(siteInfo.siteroot)}}/addons/yf_chengyujiekong/resource/image/component/popupZhuangyuanbang/wdlxz.png"></image>
                </view>
            </view>
        </view>
    </view>
</popup>
<wxs module="fn" src="../../common.wxs" />