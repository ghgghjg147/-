<view bindtap="openOtherGame" class="new-more-game">
    <swiper autoplay="{{true}}" bindchange="change" circular="{{true}}" class="new-more-game-swiper" duration="{{200}}" interval="{{3000}}">
        <swiper-item catchtouchmove="catchtouchmove" wx:for="{{moreGameList}}" wx:key="{{index}}">
            <image src="{{item.gameIcon}}" style="width: 100%;height: 100%;"></image>
        </swiper-item>
    </swiper>
    <view class="new-more-game-text-arrow">
        <view class="new-more-game-text">点击试玩</view>
        <view class="new-more-game-arrow"></view>
    </view>
</view>
