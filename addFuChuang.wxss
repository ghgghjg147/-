.btn_default {
    background: transparent;
    padding: 0;
    margin: 0;
    border: 0;
    border-radius: 0;
    text-align: left;
    line-height: normal;
    overflow: visible;
}

.btn_default::after {
    border: none;
}

.no-energy-popup {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.title-img {
    width: 401rpx;
    height: 67rpx;
    margin-top: 19rpx;
}

.title-content {
    width: 600rpx;
    height: 655rpx;
    display: block;
    margin: 21rpx auto 100rpx;
}

.popup-title {
    position: relative;
    width: 568rpx;
    height: 116rpx;
    margin: 26rpx auto;
}

.popup-title-bg {
    width: 100%;
    height: 100%;
    display: block;
}

.popup-title-text {
    width: 408rpx;
    height: 56rpx;
    display: block;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.close-btn {
    width: 60rpx;
    height: 60rpx;
    display: block;
    position: absolute;
    right: 0;
    top: 0;
    transform: translate(50%,-50%);
}