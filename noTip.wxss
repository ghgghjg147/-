.btn_default {
    background: transparent;
    padding: 0;
    margin: 0;
    border: 0;
    border-radius: 0;
    text-align: left;
    line-height: normal;
    overflow: visible;
}

.btn_default::after {
    border: none;
}

.no-energy-popup {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.title-text {
    text-align: center;
    font-size: 64rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(72,53,38,1);
    margin: 25rpx 0 0;
}

.desc {
    font-size: 40rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    color: rgba(117,92,73,1);
    text-align: center;
    margin: 66rpx 0 33rpx;
}

.content {
    width: 610rpx;
    height: 157rpx;
    background: rgba(78,59,44,1);
    border-radius: 20rpx;
    margin-top: 33rpx;
    margin-bottom: 20rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.content.shop {
    flex-direction: column;
    height: 225rpx;
}

.content-title {
    width: 610rpx;
    height: 68rpx;
    background: rgba(72,53,38,1);
    border-radius: 20rpx 20rpx 0 0;
    font-size: 30rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    color: rgba(253,246,230,1);
    display: flex;
    justify-content: center;
    align-items: center;
    border-bottom: 2rpx rgba(114,91,73,1) solid;
}

.content-box {
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
    flex: 1;
}

.content:last-child {
    margin-top: 0;
    margin-bottom: 53rpx;
}

.left {
    flex: 1;
}

.top {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 48rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    color: rgba(255,255,255,1);
}

.bottom {
    font-size: 30rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    color: rgba(255,255,255,1);
    opacity: 0.5;
    margin-top: 13rpx;
}

.tip-icon {
    width: 60rpx;
    height: 60rpx;
    display: block;
}

.button {
    width: 260rpx;
    height: 80rpx;
    margin-right: 24rpx;
    background: rgba(47,204,204,1);
    box-shadow: 0px 8rpx 0px 0px rgba(19,124,124,1);
    border-radius: 40rpx;
    font-size: 36rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    color: rgba(255,255,255,1);
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 1;
}

.button.shop {
    background: rgba(255,177,42,1);
    box-shadow: 0px 8rpx 0px 0px rgba(191,117,19,1);
}

.btn-icon {
    width: 44rpx;
    height: 44rpx;
    display: block;
    margin-right: 4rpx;
}

.cur-price {
    font-size: 36rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    color: rgba(255,255,255,1);
}

.origin-price {
    font-size: 26rpx;
    font-family: PingFang-SC-Heavy;
    font-weight: 800;
    text-decoration: line-through;
    color: rgba(191,117,19,1);
    margin-left: 9rpx;
}

.days-sign-popup-title {
    width: 375rpx;
    height: 63rpx;
    display: block;
    margin: 16rpx auto 38rpx;
}

.tip-container {
    position: relative;
    width: 600rpx;
    margin: auto;
}

.tip-bg1 {
    width: 600rpx;
    height: 100rpx;
    margin: 0 auto 12rpx;
}

.tip-bg2 {
    width: 600rpx;
    height: 518rpx;
    margin: 0 auto 40rpx;
}

.tip-content,.tip-content2 {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    font-size: 32rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(72,53,38,1);
    box-sizing: border-box;
    padding: 35rpx 20rpx;
    line-height: 1;
    white-space: nowrap;
}

.tip-img {
    width: 375rpx;
    height: 326rpx;
    display: block;
    margin: 35rpx auto 0;
}

.sign-up-container {
    width: 308rpx;
    height: 112rpx;
    margin: 0 auto 37rpx;
}

.sign-up-btn {
    width: 308rpx;
    height: 112rpx;
    display: block;
}